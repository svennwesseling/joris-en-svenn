//rnwy_comp: dmp/v2/dmp

$RNWY.loader()
	.now('jquery')
	.now('webshop')
	.win(function () {
		var isProduction = true;
		window.Krux||((Krux=function(){Krux.q.push(arguments)}).q=[]);
		window.Krux.taxonomy = getTaxonomyObj();
		if (isProduction) {
			function loadKrux() {
				var s = document.getElementsByTagName('script')[0];
				var k = document.createElement('script');
				k.type = 'text/javascript';
				k.id = 'krux-script';
				k.defer = true;
				k.src = 'https://cdn.krxd.net/controltag/q7lklamxv.js';
				s.parentNode.insertBefore(k, s);

				if (window.sessionStorage.getItem('setKruxConsent') === null) {
					window.sessionStorage.setItem('setKruxConsent', 'hasFired');
					window.Krux('ns:bolcom', 'consent:set', {dc: true, al: true, tg: true, cd: true, sh: false, re: false});
				}
			}
			if ('requestIdleCallback' in window) {
				window.requestIdleCallback(loadKrux);
			} else {
				setTimeout(loadKrux,10);
			}
		}
	});

function getTaxonomyObj() {
	var dataElement = document.getElementById('js_taxonomy_data_1647d520-3543-4f51-ad60-09100586dd18');
	var taxonomyObj = JSON.parse(dataElement.textContent);
	if (taxonomyObj) {
		var taxonomyObjForPage = taxonomyObj.pdpTaxonomyObj || taxonomyObj.homePageTaxonomyObj || taxonomyObj.listPageTaxonomyObj || {};
		return transformTaxonomyForKrux(taxonomyObjForPage);
	}
	return {};
}

// Because rnwy doesn't support all json data structures, we have to do some manual stitching here.
function transformTaxonomyForKrux(taxonomy) {
	if (taxonomy && taxonomy.productInfo) {
		taxonomy.productInfo = taxonomy.productInfo.map(function (pInfo) {
			pInfo.price = parseFloat(pInfo.price);
			if (pInfo.averageReviewRating) {
				pInfo.averageReviewRating = parseFloat(pInfo.averageReviewRating);
			}
			pInfo.categoryTreeList = pInfo.categoryTreeList.map(function (categoryTree) { return categoryTree.tree });
			return pInfo;
		});
	}
	return taxonomy;
}


//rnwy_comp: analytics/v2/analytics
window['@wsp/analytics-loader'] = window['@wsp/analytics-loader'] || {};
window['@wsp/analytics-loader'].waitForConsent = true;
window['@wsp/analytics-loader'].fbPixId = '903451823449917';

$RNWY.loader()
    .win('analytics-loader')
    .win(function () {
        try {
            var parsedData = getAnalyticsObj();
            if (parsedData) {
                
                    WSP.utils.Consent.get('third-party-cookies').then(function (consent) {
                        if (consent) {
                            window['@wsp/analytics-loader'].globalSiteTag.init(parsedData);
                            window['@wsp/analytics-loader'].facebookPixel.init(parsedData);
                        }
                    });
                
            }
        } catch(error) {
            console.error(error);
        }
    });

function getAnalyticsObj() {
    var dataElement = document.getElementById('js_analytics_data_d315c5f4-9c7b-449d-9763-0097b473753e');
    var analyticsObj = JSON.parse(dataElement.textContent);
    if (analyticsObj) {
        analyticsObjForPage = analyticsObj.pdpAnalyticsObj || analyticsObj.listPageAnalyticsObj || analyticsObj.simplePageAnalyticsObj || {};
        return transformAnalyticsObj(analyticsObjForPage);
    }
    return {};
}

// Because rnwy doesn't support all json data structures, we have to do some manual stitching here.
function transformAnalyticsObj(analyticsObj) {
    if (analyticsObj && analyticsObj.product && analyticsObj.product.price) {
        analyticsObj.product.price = parseFloat(analyticsObj.product.price);
    }
    return analyticsObj;
}



}());</script>
</body>
